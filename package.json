{
  "name": "sample-telegram-bot",
  "version": "1.0.0",
  "description": "A learning tool for how to make a bot on telegram",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --sequential compile server",
    "develop": "npm-run-all --parallel compile-watch server-watch",
    "compile": ".\\node_modules\\.bin\\tsc.cmd -p .\\tsconfig.json",
    "compile-watch": ".\\node_modules\\.bin\\tsc.cmd --watch -p .\\tsconfig.json",
    "server": "node dist/index.js",
    "server-watch": "nodemon --inspect dist/index.js",
    "debug": "node --inspect dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j-c-levin/sample-telegram-bot.git"
  },
  "author": "Joshua Levin",
  "license": "MIT License",
  "bugs": {
    "url": "https://github.com/j-c-levin/sample-telegram-bot/issues"
  },
  "homepage": "https://github.com/j-c-levin/sample-telegram-bot#readme",
  "dependencies": {
    "telegraf": "^3.20.0"
  },
  "devDependencies": {
    "dotenv": "^5.0.1",
    "ngrok": "^3.0.1",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.2",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  }
}
